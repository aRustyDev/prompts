{
    "mcpServers": {
        "github": {
          "url": "https://api.githubcopilot.com/mcp/",
          "authorization_token": "Bearer <your GitHub PAT>"
        }
        "github": {
            "command": "docker",
            "args": [
                "run",
                "-i",
                "--rm",
                "--env",
                "GITHUB_PERSONAL_ACCESS_TOKEN",
                "ghcr.io/github/github-mcp-server"
            ],
            "env": {
                "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PAT}",
                "MCP_GITHUB_AVAILABLE": "true",
                "LOG_LEVEL": "${MCP_LOG_LEVEL:-info}",
                "CACHE_DIR": "${XDG_CACHE_HOME:-${HOME}/.cache}/mcp"
            }
        },
        "project-specific-tool": {
            "command": "${HOME}/.mcp/my-mcp-server",
            "args": ["--project", "${PWD}"],
            "env": {
                "PROJECT_CONFIG": "${PWD}/.config"
            }
        },
        "uiTesting": {
            "command": "${HOME}/.mcp/uiTesting",
            "args": ["--project", "${PWD}"],
            "env": {
                "PROJECT_CONFIG": "${PWD}/.config"
            }
        },
        "filesystem": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-filesystem",
                "/path/to/allowed/directory"
            ]
        },
        "git": {
            "command": "docker",
            "args": [
                "run",
                "--rm",
                "-i",
                "--mount",
                "type=bind,src=${workspaceFolder},dst=/workspace",
                "mcp/git"
            ]
        },
        "memory": {
            "command": "docker",
            "args": [
                "run",
                "-i",
                "-v",
                "claude-memory:/app/dist",
                "--rm",
                "mcp/memory"
            ]
        },
        "sequentialthinking": {
            "command": "docker",
            "args": ["run", "--rm", "-i", "mcp/sequentialthinking"]
        },
        "time": {
            "command": "docker",
            "args": ["run", "-i", "--rm", "mcp/time"]
        },
        "postgres": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-postgres",
                "${DATABASE_URL:-postgresql://localhost/mydb}"
            ]
        }
    }
}
